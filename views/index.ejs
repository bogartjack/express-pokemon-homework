<!Doctype html>
<html>
<head> 
	<meta charset='utf-8'>
	<title></title>
</head>
<body>
	<h1>ALL THE POKEMON</h1>
	<a href='/pokemon/new'>Make a new one</a>
	<ul class='pokemon'>
		<% for(let i =0;i<pokemon.length;i++){%>
			<li><a href='/pokemon/<%=i%>'>
				<%=pokemon[i].name;%>
			</a><br/>
			<img src='<%=pokemon[i].img%>'></img><br/>
			<a href='/pokemon/<%=i%>/edit'>edit</a><br/>
			<form action='/pokemon/<%=i;%>?_method=DELETE' method='POST'>
				<input type='submit' value='DELETE'>
			</form><br/>
			</li>
		<% } %>
	</ul>
</body>
</html>
