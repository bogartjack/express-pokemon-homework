<!Doctype html>
<html>
<head> 
	<meta charset='utf-8'>
	<title></title>
</head>
<body>
	<h1>ALL THE POKEMON</h1>
	<ul class='pokemon'>
		<% for(let i =0;i<pokemon.length;i++){%>
			<li><a href='/pokemon/<%=i%>'>
				<%=pokemon[i].name;%>
			</a><br/>
			<img src='<%=pokemon[i].img%>'></img>
			</li>
		<% } %>
	</ul>
</body>
</html>
